# Anthropic API Key
# Get from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# EnrichLayer API Key
# Get from: https://enrichlayer.com/dashboard/api-keys
ENRICHLAYER_API_KEY=your_enrichlayer_api_key_here

# Laminar Observability (recommended for full visibility)
# Get API key from: https://www.laminar.run
# Laminar uses a Rust proxy to capture ALL SDK communications
LAMINAR_API_KEY=your_laminar_api_key_here

# Debug Mode (optional - shows full LLM prompts/responses in console)
# Set to "true" to see exactly what the LLM sees and produces
# Great for learning how agents work under the hood!
DEBUG_LLM=false

# Auto-feedback mode (for non-interactive demos)
# Set to "true" to skip human review prompts and use sample feedback
AUTO_FEEDBACK=false
